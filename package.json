{
  "name": "tsdown-dual-publish-esm-cjs",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "test": "pnpm run build && attw --pack ."
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "tsdown": "^0.14.2",
    "typescript": "~5.8.3"
  }
}
